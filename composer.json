{
  "name": "contributte/code-rules",
  "description": "Codesniffer & phpstan prepared configs for easy-to-use. Rewritten by contributte's authors.",
  "keywords": [
    "phpstan",
    "codesniffer",
    "rules",
    "analysis"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/code-rules",
  "authors": [
    {
      "name": "Pavel Janda",
      "homepage": "https://paveljanda.com"
    },
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "phpstan/phpstan": "^1.9.4",
    "phpstan/phpstan-strict-rules": "^1.4.4",
    "phpstan/phpstan-dibi": "^1.0.1",
    "slevomat/coding-standard": "^8.14.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  }
}
